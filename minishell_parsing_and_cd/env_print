#include "minishell.h"

//this will print the linked list
int	ft_print_env_lst(t_env_list *env)
{
	t_env_list	*node;

	node = env;
	if (node == NULL)
		return (1);
	while (node)
	{
		printf("%s=%s\n", node->name, node->value);
		node = node->next;
	}
	return (0);
}